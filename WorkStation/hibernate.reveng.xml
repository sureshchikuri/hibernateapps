<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="ZONE" schema="SYSTEM">
		<column name="FLOOR_ID" exclude="true"></column>
	</table>
	<table name="FLOOR" schema="SYSTEM">
		<primary-key>
			<key-column name="FLOOR_ID" property="floorId" />
		</primary-key>
		<column name="FLOOR_NAME" property="floorName"></column>
		<column name="FLOOR_DESC" property="floorDesc"></column>
	</table>
<table name="FLOORXTRA" schema="SYSTEM">
	<primary-key>
		<key-column name="FLOOR_ID" property="floorId"/>
	</primary-key>
</table>
</hibernate-reverse-engineering>